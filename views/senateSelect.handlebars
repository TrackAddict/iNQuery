<meta charset="utf-8">
  <title>jQuery UI Autocomplete - Default functionality</title>
  <link rel="stylesheet" href="css/jquery-ui.css">
  <script src="js/jquery-1.9.1.js"></script>
  <script src="js/jquery-ui.js"></script>
  <script>
  $(function() {
    var availableTags = [
      "Barbra Boxer (Dem - CA)",
      "Diane Feinstein (Dem - CA)",
      "Dean Heller (Rep - NV)",
      "Harry Reid (Dem - NV)",
      "Jeff Flake (Rep - AZ)",
      "John McCain (Rep - AZ)",
      "Jeff Merkley (Dem - OR)",
      "Ron Wyden (Dem - OR)"
    ];

    $( "#names1" ).autocomplete({
      source: availableTags
    });

    $( "#names2" ).autocomplete({
      source: availableTags
    });

    $( "#find" ).autocomplete({
      source: availableTags
    });

  });
  </script>


</head>

<body>

  <a name = "top">

  <nav class="navbar navbar-default" role="navigation">
    <div class="container-fluid">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>

        </button>
        <a class="navbar-brand" href="/">iNQuery</a>
      </div>

      <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav">
          <li><a href="/">Home</a></li>
          <li class="active"><a href="#">Compare Records</a></li>
          <li><a href="lawsgallery">Laws Gallery</a></li>
          <li><a href="gallery">Senators Gallery</a></li>
          <li><a href="help">User FAQ Guide</a></li>
        <form class="navbar-form navbar-left" role="search">
          <div class="form-group">
            <input type="text" class="form-control" id = "find" placeholder="Find Representative">
          </div>
          <button type="submit" class="btn btn-default">Submit</button>
        </form>
        
      </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
  </nav>

  <div class="container">
    <!-- h1 through h6 are headers. The higher the number, the smaller the header -->
    <!-- p means paragraph -->
    <div class="jumbotron">
      <h2 class="inContainer">Comparison: Senators</h2>
      <h4 class="inContainer">Two current Senators &middot; One central issue</h4>
      <!-- <button id="testjs" class="btn btn-primary btn-lg">Test Javascript</button> -->
    </div>

  <form id="projectForm" role="form">



  

  <h4 class="bigh3">Compare Senators:</h4>
    <div class="form-group">
      <label for="value">First Senator:</label>
      <input type="value" class="form-control" id="names1" placeholder="Name, State, or Party">
    </div>

    <div class="form-group">
      <label for="value">Second Senator:</label>
      <input type="value" class="form-control" id="names2" placeholder="Name, State, or Party">
    </div>





    <h4 class="bigh3">On This Issue:</h4>
    <div class="radio">
      <label>
        <input type="radio" name="optionsRadios" id="optionsRadios1" value="#project1" checked>
        Federal Spending
      </label>
    </div>
    <div class="radio">
      <label>
        <input type="radio" name="optionsRadios" id="optionsRadios2" value="#project2">
        Education
      </label>
    </div>
    <div class="radio">
      <label>
        <input type="radio" name="optionsRadios" id="optionsRadios2" value="#project6">
        Tax Policy
      </label>
    </div>

    <div class="span7 text-center">
    <a href="results"><button id="submitBtn" type="button" class="btn btn-default">Submit</button></a>
    </div>
  </form>
  <hr>
  <ul class="pager">
  <li class="previous"><a href="javascript:history.go(-1)">&larr; Back</a></li>
  <li><a href="#top">&uarr; Top</a></li>

  </ul>

  <script src="../js/bootstrap.min.js"></script>
  <script src = "../js/iNQuery.js"></script>



   
